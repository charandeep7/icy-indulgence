// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  // provider = "mysql"
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Flavors {
  id Int @id @default(autoincrement())
  type String
  title String
  img String
  alt String
  price Float
}
// as soon as i add to flavors i need to add also in searchquery table 
model SearchQuery {
  id Int @id @default(autoincrement())
  query String
  path String
}
